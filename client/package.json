    "generate:schema": "protoc --proto_path=../proto --ts_out=./src/schema2 ../proto/*.proto"
  },
  "dependencies": {
    "@pixi/react": "^7.1.2",
    "@protobuf-ts/grpcweb-transport": "^2.9.4",
    "antd": "^5.23.1",
