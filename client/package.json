{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix --cache",
    "lint:nofix": "eslint . --cache",
    "format": "prettier --write . --cache",
    "format:nofix": "prettier --check . --cache",
    "typecheck": "tsc -b --noEmit",
    "preview": "vite preview",
    "generate:schema": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/schema --proto_path=../proto ../proto/*.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@pixi/react": "^7.1.2",
    "antd": "^5.23.1",
    "pixi.js": "^7.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-proto": "^2.6.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
